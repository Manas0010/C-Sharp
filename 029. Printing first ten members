// 9. Write a program that prints the first 10 members
// of the sequence:-1, 2, -3, 4, -5, 6, -7, ...
using System;
namespace numberPrinting
{
    class Program
    {
        static void Main()
        {
            Console.Title="Prints first 10 numbers...";
            Console.SetWindowSize(100, 20);
            //Console.SetBufferSize(80, 40);
            for(int i = 1; i < 11; i++)
            {
                Console.WriteLine(i % 2 == 0 ? i : -i);
            }
        }
    }
}
